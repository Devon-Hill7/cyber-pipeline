{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www --ignore vue",
    "start": "NODE_ENV=production node ./bin/www",
    "reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "test": "mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "cov": "nyc --reporter=html mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "test-json": "mocha --require test/hooks.js --recursive --timeout 2000 --exit --reporter json > test-results.json"
  },
  "dependencies": {
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "node-cas-authentication": "^0.0.15",
    "nodemon": "^3.0.3",
    "objection": "^3.1.3",
    "pug": "^3.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^5.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
}
